APP_NAME="Propaganda Analysis System"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Duomenų bazės nustatymai
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=propaganda_analysis
DB_USERNAME=root
DB_PASSWORD=

# Broadcast, Cache, Queue, Session konfigūracija (Redis BŪTINA)
BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DRIVER=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120

# Redis konfigūracija
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# LLM API raktai
CLAUDE_API_KEY=your_claude_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# API konfigūracija
MAX_CONCURRENT_REQUESTS=10
RETRY_ATTEMPTS=3
REQUEST_TIMEOUT=60

# Rate limiting nustatymai
CLAUDE_RATE_LIMIT=50
GEMINI_RATE_LIMIT=50  
OPENAI_RATE_LIMIT=50

# Autentifikavimo nustatymai
# Formato pavyzdys: AUTH_USERS=username1:password1,username2:password2
AUTH_USERS=admin:password,user1:password1,user2:password2